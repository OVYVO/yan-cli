{
  "include": ["src"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "outDir": "./bin",
    "target": "ES5",
    "paths": {
      "@":["./src"],
      "@utils":["./utils"],
      "@config":["./config"],
      "@command":["./command"]
    }
  }
}