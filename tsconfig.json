{
  "include": ["lib/*"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "outDir": "./bin",
    "target": "ES5",
    "baseUrl": "./",
    "paths": {
      "@/*":["lib/*"],
      "@template/*":["lib/template/*"],
      "@utils/*":["lib/utils/*"],
      "@config/*":["lib/config/*"],
      "@core/*":["lib/core/*"]
    }
  }
}