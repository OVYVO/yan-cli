{
  "include": ["src/*"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "outDir": "./bin",
    "target": "ES5",
    "baseUrl": "./",
    "paths": {
      "@/*":["src/*"],
      "@utils/*":["src/utils/*"],
      "@config/*":["src/config/*"],
      "@core/*":["src/core/*"]
    }
  }
}